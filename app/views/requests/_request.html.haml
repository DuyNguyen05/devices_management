%tr
  %td
    = l(request.created_at, format: :date_month_year_concise)
  %td
    - if request.end_at.present?
      = l(request.end_at, format: :date_month_year_concise)
  %td
    = request.user.name
  %td
    = link_to t(".edit"), edit_request_path(request), class: "btn btn-info btn-sm", remote: true
