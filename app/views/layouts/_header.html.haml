%header.navbar.navbar-fixed-top.navbar-inverse
  .container
    %nav
      %ul.nav.navbar-nav.navbar-right
        %li= redirect_to root_path
        - if logged_in?
          %li= link_to "Log out", logout_path, method: :delete
          %li.dropdown
        - else
        %li= link_to "Log in", login_path