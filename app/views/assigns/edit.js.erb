$("#ModalAssignEdit").html("<%= j render "assigns/edit", assign: @assign, users: @users %>");
$("#myModalAssignEdit").modal("show");
$("#autocomplete1").autocomplete({
  source: function(request, response){
    $.get('/search', { query: request.term }, function(data) {
      response(data.users);
    });
  }
});
$("#autocomplete1").autocomplete("option", "appendTo", ".edit_request");
